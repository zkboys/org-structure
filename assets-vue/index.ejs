  <link href="<%= staticFile('sb-admin-v2/css/plugins/timeline/timeline.css') %>" rel="stylesheet">

  <!-- SB Admin CSS - Include with every page -->
  <link href="<%= staticFile('sb-admin-v2/css/sb-admin.css') %>" rel="stylesheet">
</head>
<body>
<div id="app"></div>
<!-- Core Scripts - Include with every page -->
<script src="<%= staticFile('sb-admin-v2/js/jquery-1.10.2.js') %>"></script>
