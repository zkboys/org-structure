  <title><%= title %></title>
  <!-- Core CSS - Include with every page -->
  <link href="<%= staticFile('sb-admin-v2/css/bootstrap.min.css') %>" rel="stylesheet">
  <link href="<%= staticFile('sb-admin-v2/font-awesome/css/font-awesome.css') %>" rel="stylesheet">

  <!-- Page-Level Plugin CSS - Dashboard -->
